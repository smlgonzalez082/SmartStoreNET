<section class="footer-contact-area section_padding_100 clearfix" id="contact">
    <div class="container">
        <div class="row">
            <div class="col-md-6">
                <!-- Heading Text  -->
                <div class="section-heading">
                    <h2>Get in touch with us!</h2>
                    <div class="line-shape"></div>
                </div>
                <div class="footer-text">
                    <p>Contact us to find out how FloFilters can work for you.</p>
                </div>
                <!--  <div class="address-text">
                    <p><span>Address:</span> 40 Baria Sreet 133/2 NewYork City, US</p>
                </div>   -->
                <div class="phone-text">
                    <p><span>Phone:</span> <a style="color: #726a84;font-size: 16px;font-weight: 300;" href="tel:+1 (305) 562-6960">+1 (305) 562-6960</a></p>
                </div>
                <div class="email-text">
                    <p><span>Email:</span> <a style="color: #726a84;font-size: 16px;font-weight: 300;" href="mailto:sales@flofilters.com">sales&#64;flofilters.com</a></p>
                </div>
            </div>
            <div class="col-md-6">
                <!-- Form Start-->
                <div class="contact_from">

                    @if (Model.SuccessfullySent)
                    {
                        <div class="alert alert-success">
                            @Model.Result
                        </div>
                    }
                    else
                    {
                        using (Html.BeginRouteForm("ContactUs", FormMethod.Post, new { @class = "contact-form" }))
                        {
                            if (!ViewData.ModelState.IsValid)
                            {
                                @Html.ValidationSummary(false)
                            }

                            if (this.EnableHoneypotProtection)
                            {
                                <div class="d-none">
                                    @Html.HoneypotField()
                                </div>
                            }
                            <!-- Message Input Area Start -->
                            <div class="contact_input_area">
                                <div class="row">
                                    <!-- Single Input Area Start -->
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            @Html.TextBoxFor(model => model.FullName, new { @required = Model.FullNameRequired, @placeholder = "Your Name *", @class = "form-control" })
                                        </div>
                                    </div>
                                    <!-- Single Input Area Start -->
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            @Html.TextBoxFor(model => model.Email, new { @required = Model.FullNameRequired, @placeholder = "Your E-mail *", @class = "form-control" })
                                        </div>
                                    </div>
                                    <!-- Single Input Area Start -->
                                    <div class="col-12">
                                        <div class="form-group">
                                            @Html.TextAreaFor(model => model.Enquiry, new { @class = "form-control", cols = "30", rows = "4", placeholder = "Your Message *" })
                                            @Html.ValidationMessageFor(model => model.Enquiry)
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="row justify-content-end">
                                        <div class="col-sm-9">
                                            @{ Html.RenderWidget("gdpr_consent"); }
                                        </div>
                                    </div>

                                    <!-- Single Input Area Start -->
                                    <div class="col-12">
                                        <button type="submit" name="send-email" class="btn submit-btn">
                                            Send Now
                                        </button>
                                    </div>
                                </div>
                            </div>
                            <!-- Message Input Area End -->
                        }
                    }
                </div>
            </div>
        </div>
    </div>
</section>
