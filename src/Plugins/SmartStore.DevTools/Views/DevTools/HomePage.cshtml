<!-- ***** Wellcome Area Start ***** -->

@{
    var products = (ProductSummaryModel)ViewBag.Products;
}

<section class="wellcome_area clearfix" id="home">
    <div class="container h-100">
        <div class="row h-100 align-items-center">
            <div class="col-12 col-md">
                <div class="wellcome-heading">
                    <h2>Pure air is just a few clicks away...</h2>
                </div>
                <div class="get-start-area">
                    <form action="#" method="post" class="form-inline">
                        <a href='/MyCheckout/QualityStep'
                           class="submit">
                            Get Started
                        </a>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <div class="welcome-thumb wow fadeInDown hidden-filter" data-wow-delay="0.5s">
        <img src="~/Content/Images/homepage/bg-img/welcome-img3.png" alt="">
    </div>
</section>
<!-- ***** Wellcome Area End ***** -->
<!-- ***** Awesome Features Start ***** -->
<section class="awesome-feature-area bg-white section_padding_100 clearfix" id="whyus">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <!-- Heading Text -->
                <div class="section-heading text-center">
                    <h2>Why choosing FloFilters</h2>
                    <div class="line-shape"></div>
                </div>
            </div>
        </div>

        <div class="row">
            <!-- Single Feature Start -->
            <div class="col-12 col-sm-6 col-lg-4">
                <div class="single-feature">
                    <i class="fa fa-truck" aria-hidden="true"></i>
                    <h5>Ships Free</h5>
                    <p>We offer free shipping for every single one of your orders.</p>
                </div>
            </div>
            <!-- Single Feature Start -->
            <div class="col-12 col-sm-6 col-lg-4">
                <div class="single-feature">
                    <i class="fa fa-people-carry" aria-hidden="true"></i>
                    <h5>Delivered to your door</h5>
                    <p>No need to leave your home, we'll that for you.</p>
                </div>
            </div>
            <!-- Single Feature Start -->
            <div class="col-12 col-sm-6 col-lg-4">
                <div class="single-feature">
                    <i class="fa fa-hand-holding-usd" aria-hidden="true"></i>
                    <h5>Affordable</h5>
                    <p>We have the right price, no more retail prices for you.</p>
                </div>
            </div>
            <!-- Single Feature Start -->
            <div class="col-12 col-sm-6 col-lg-4">
                <div class="single-feature">
                    <i class="fas fa-bell" aria-hidden="true"></i>
                    <h5>We never forget</h5>
                    <p>We will remind you when to change your filters. Relax, we've got your back.</p>
                </div>
            </div>
            <!-- Single Feature Start -->
            <div class="col-12 col-sm-6 col-lg-4">
                <div class="single-feature">
                    <i class="fa fa-magic" aria-hidden="true"></i>
                    <h5>Fast & Simple</h5>
                    <p>You sing up and we do the rest. Enjoy some free time</p>
                </div>
            </div>
            <!-- Single Feature Start -->
            <div class="col-12 col-sm-6 col-lg-4">
                <div class="single-feature">
                    <i class="fa fa-vector-square" aria-hidden="true"></i>
                    <h5>All Filter Sizes</h5>
                    <p>Whatever size you need, we got it covered.</p>
                </div>
            </div>
        </div>

    </div>
</section>
<!-- ***** Awesome Features End ***** -->
<!-- ***** Pricing Plane Area Start *****==== -->
<section class="pricing-plane-area section_padding_100_70 clearfix" id="qualities">
